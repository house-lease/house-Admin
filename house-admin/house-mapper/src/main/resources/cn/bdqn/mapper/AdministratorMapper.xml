<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.bdqn.mapper.AdministratorMapper" >

    <select id="selectByNameAndPassword" resultType="cn.bdqn.domain.Administrator">
        select id, name , password from t_administrator where name = #{name} and password = #{password}
    </select>

</mapper>